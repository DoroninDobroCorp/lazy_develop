def get_file_hash(filepath):
    """–í—ã—á–∏—Å–ª—è–µ—Ç SHA256 —Ö—ç—à —Ñ–∞–π–ª–∞."""
    if not os.path.exists(filepath) or os.path.isdir(filepath): return None
    h = hashlib.sha256()
    with open(filepath, 'rb') as f:
        while True:
            chunk = f.read(8192)
            if not chunk: break
            h.update(chunk)
    return h.hexdigest()

